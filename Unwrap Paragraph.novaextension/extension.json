{
    "bugs": "https://github.com/gingerbeardman/Unwrap-Paragraph/issues",
    "description": "Merges multiple lines of select text into one",
    "funding": "https://ko-fi.com/gingerbeardman",
    "identifier": "com.gingerbeardman.unwraptext",
    "homepage": "https://www.gingerbeardman.com",
    "name": "Unwrap Paragraph",
    "organization": "gingerbeardman",
    "repository": "https://github.com/gingerbeardman/Unwrap-Paragraph",
    "version": "1.0.1",

    "categories": ["commands"],

    "main": "main.js",

    "commands": {
        "editor": [
            {
                "title": "Unwrap Paragraph",
                "command": "com.gingerbeardman.unwraptext",
                "shortcut": "control-q",
                "when": "editorHasSelection"
            }
        ]
    }
}
